{
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.surround": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "f"],
      "after": ["<esc>"]
    },
    {
      "before": ["f", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "<space>"],
      "after": ["o", "<esc>", "k"]
    },
    {
      "before": ["[", "<space>"],
      "after": ["O", "<esc>", "j"]
    },
    {
      "before": ["g", "j"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["g", "k"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["<c-l>"],
      "after": ["<c-w>", "<c-w>"]
    },
    {
      "before": ["<c-h>"],
      "after": ["<c-w>", "<c-w>"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "window.zoomLevel": 0,
  "window.menuBarVisibility": "toggle",
  // "explorer.openEditors.visible": 0,
  "editor.fontFamily": "'Fantasque Sans Mono', 'JetBrains Mono', Consolas, 'Fira Code Retina'",
  "editor.fontSize": 15,
  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.7,
  "editor.fontWeight": "100",
  "terminal.integrated.fontFamily": "'Iosevka SS04', 'Fira Code Retina'",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.allowChords": false,
  "python.jediEnabled": false,
  "window.title": "${dirty}${activeEditorLong}${separator}${appName}",
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.parameterHints.enabled": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "html.suggest.html5": false,
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wrappingIndent": "same",
    "editor.tabSize": 4,
    "editor.rulers": [80]
  },
  "[mdx]": {
    "editor.rulers": [80]
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.rulers": [100]
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.rulers": [100]
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.rulers": [100]
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.rulers": [100]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSave": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "workbench.activityBar.visible": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultDateFormat": "MMMM Do, YYYY HH:MM",
  "editor.foldingStrategy": "indentation",
  "scm.diffDecorationsGutterWidth": 2,
  "problems.showCurrentInStatus": true,
  "search.enableSearchEditorPreview": true,
  "editor.semanticHighlighting.enabled": true,
  "workbench.view.experimental.allowMovingToNewContainer": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "go.formatTool": "goimports",
  "dart.openDevTools": "flutter",
  "workbench.colorTheme": "One Dark Pro",
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "workbench.startupEditor": "newUntitledFile",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  }
}
